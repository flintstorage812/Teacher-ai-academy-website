{
  "name": "teacher-ai-academy-backend",
  "version": "1.0.0",
  "description": "Backend API for Teacher AI Academy blog with n8n integration",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/src/server.js",
    "migrate": "tsx prisma/migrate.ts",
    "seed": "tsx prisma/seed.ts",
    "setup": "node setup.js",
    "test": "node test-api.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["blog", "api", "n8n", "education", "ai"],
  "author": "Teacher AI Academy",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "marked": "^11.1.1",
    "prisma": "^5.7.1",
    "rss": "^1.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
